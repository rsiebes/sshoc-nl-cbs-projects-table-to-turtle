{
  "zotero": {
    "api_key": "your_zotero_api_key_here",
    "user_id": "your_zotero_user_id",
    "library_type": "user"
  },
  "cbs": {
    "api_base_url": "https://opendata.cbs.nl/ODataApi/odata",
    "timeout": 30,
    "max_retries": 3
  },
  "enrichment": {
    "batch_size": 100,
    "output_format": "json",
    "include_metadata": true
  },
  "logging": {
    "level": "INFO",
    "file": "logs/enrichment.log"
  }
}

